<UserControl x:Class="Viewers.UserControls.ReturnButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ctrl="clr-namespace:GestureControls.Controls;assembly=GestureControls"
             mc:Ignorable="d"
             d:DesignHeight="75" d:DesignWidth="95">
    <Canvas>
        <Image Name="Imagen" Width="95" Height="75" Opacity=".10" Source="ButtonPictures/ReturnButton.png"/>
        <Image Name="OnHover" Width="95" Height="75" Opacity="0" Source="ButtonPictures/BackButton.png"/>
        <ctrl:HoverButton Name="Boton" Width="95" Height="75" Opacity="0"
                          KinectCursorEnter="MakeVisible" KinectCursorLeave="MakeInvisible"/>
    </Canvas>
</UserControl>
